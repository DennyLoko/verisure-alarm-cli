{
    "name": "pedrommone/verisure-alarm-cli",
    "type": "project",
    "description": "Tiny CLI to enable/disable verisure alarm",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/console": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/dom-crawler": "^5.3",
        "symfony/dotenv": "^5.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
